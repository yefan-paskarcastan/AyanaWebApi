@page "/"
@namespace TegreiUI.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>TegreiUI</title>
    <base href="~/" />
    @*<link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />*@
    <link href="css/site.css" rel="stylesheet" />

    <link rel="stylesheet" href="studio/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto"/>
    <link rel="stylesheet" href="studio/fonts/fontawesome-all.min.css"/>
    <link rel="stylesheet" href="studio/fonts/font-awesome.min.css"/>
    <link rel="stylesheet" href="studio/fonts/ionicons.min.css"/>
    <link rel="stylesheet" href="studio/fonts/fontawesome5-overrides.min.css"/>
    <link rel="stylesheet" href="studio/css/Fixed-Header-Cruzmaltina.css"/>
    <link rel="stylesheet" href="studio/css/Footer-Dark.css"/>
    <link rel="stylesheet" href="studio/css/header-1.css"/>
    <link rel="stylesheet" href="studio/css/header-2.css"/>
    <link rel="stylesheet" href="studio/css/header.css"/>
    <link rel="stylesheet" href="studio/css/Off-Canvas-Sidebar-Drawer-Navbar.css"/>
    <link rel="stylesheet" href="studio/css/Pretty-Search-Form.css"/>
    <link rel="stylesheet" href="studio/css/styles.css"/>
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="studio/js/jquery.min.js"></script>
    <script src="studio/bootstrap/js/bootstrap.min.js"></script>
    <script src="studio/js/Off-Canvas-Sidebar-Drawer-Navbar.js"></script>
    <script src="studio/js/Off-Canvas-Sidebar-Drawer-Navbar-1.js"></script>
</body>
</html>
